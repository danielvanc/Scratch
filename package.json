{
  "name": "scratch",
  "version": "1.0.0",
  "description": "Default project starter using ReactJS and GatsbyJS",
  "main": "index.js",
  "author": "Daniel Van Cuylenburg",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "gatsby develop -H 0.0.0.0",
    "start": "gatsby develop",
    "serve": "gatsby clean && gatsby build && gatsby serve",
    "build": "gatsby clean && gatsby build",
    "analyze": "ANALYZE_BUNDLE_SIZE=true gatsby build",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier \"**/src/**/*.+(js|json|jsx)\"",
    "prettier:format": "yarn prettier --write",
    "prettier:check": "yarn prettier --list-different",
    "validate": "npm-run-all --parallel prettier:check lint build",
    "jestconf": "jest --watchAll --config tests/configs/jest.config.js",
    "jest": "jest --config tests/configs/jest.config.js",
    "jest:watch": "yarn jestconf",
    "jest:watch:cov": "yarn jestconf --collectCoverage ",
    "cypress:open": "cypress open --config-file tests/configs/cypress.json",
    "cypress:open:firefox": "cypress open --browser firefox --config-file tests/configs/cypress.json",
    "cypress:open:all": "start-server-and-test dev http://localhost:8000 cypress:open --config-file tests/configs/cypress.json",
    "cypress:run": "cypress run --config-file tests/configs/cypress.json",
    "cypress:run:all": "start-server-and-test dev http://localhost:8000 cypress:run --config-file tests/configs/cypress.json",
    "cypress:run:all:firefox": "start-server-and-test dev http://localhost:8000 cypress:open:firefox --config-file tests/configs/cypress.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn prettier:check && yarn jest"
    }
  },
  "dependencies": {
    "gatsby": "^2.20.24",
    "gatsby-cli": "^2.11.10",
    "gatsby-plugin-manifest": "^2.3.5",
    "gatsby-plugin-offline": "^3.1.4",
    "gatsby-plugin-react-helmet": "^3.2.4",
    "gatsby-plugin-sharp": "^2.5.6",
    "gatsby-source-filesystem": "^2.2.4",
    "gatsby-transformer-sharp": "^2.4.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "babel-jest": "^25.3.0",
    "babel-preset-gatsby": "^0.3.3",
    "cypress": "^4.4.0",
    "cypress-axe": "^0.8.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.10",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.6",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.0"
  }
}
